{
  "identifier": "tencent-hunyuan-image",
  "api": [
    {
      "url": "https://lobechat.tencent-hunyuan-image.230617.xyz/api/generate",
      "name": "generateImage",
      "description": "使用腾讯混元大模型，根据文本提示生成图片",
      "parameters": {
        "type": "object",
        "properties": {
          "Prompt": {
            "type": "string",
            "description": "文本描述，算法将根据输入的文本生成与之相关的图像，推荐使用中文",
            "minLength": 1,
            "maxLength": 1024
          },
          "NegativePrompt": {
            "type": "string",
            "description": "反向提示词，用于指定不希望在生成图像中出现的元素，推荐使用中文",
            "maxLength": 1024
          },
          "Style": {
            "type": "string",
            "description": "绘画风格，请在混元生图风格列表中选择期望的风格，传入风格编号",
            "example": "riman"
          },
          "Resolution": {
            "type": "string",
            "description": "生成图分辨率，支持多种尺寸比例",
            "enum": [
              "768:768",
              "768:1024",
              "1024:768",
              "1024:1024",
              "720:1280",
              "1280:720",
              "768:1280",
              "1280:768"
            ],
            "default": "1024:1024"
          },
          "Num": {
            "type": "integer",
            "description": "图片生成数量，支持1～4张",
            "minimum": 1,
            "maximum": 4,
            "default": 1
          },
          "Clarity": {
            "type": "string",
            "description": "超分选项，默认不做超分",
            "enum": ["", "x2", "x4"],
            "default": "x2"
          },
          "ContentImage": {
            "type": "object",
            "description": "用于引导内容的参考图，单边分辨率小于5000，Base64字符串小于8MB",
            "properties": {
              "ImageUrl": {
                "type": "string",
                "description": "参考图片的URL地址"
              }
            }
          },
          "Revise": {
            "type": "integer",
            "description": "prompt扩写开关，1为开启，0为关闭，默认开启",
            "enum": [0, 1],
            "default": 1
          },
          "Seed": {
            "type": "integer",
            "description": "随机种子，默认随机。正数为固定种子生成，扩写开启时固定种子不生效",
            "minimum": 0
          }
        },
        "required": ["Prompt"]
      }
    }
  ],
  "meta": {
    "avatar": "🎨",
    "description": "此插件使用腾讯混元大模型，根据文本提示生成图像。",
    "tags": ["图片", "生成", "混元", "腾讯", "hunyuan"],
    "title": "腾讯混元大模型图片生成器"
  },
  "settings": {
    "type": "object",
    "required": ["TENCENT_SECRET_ID", "TENCENT_SECRET_KEY"],
    "properties": {
      "TENCENT_SECRET_ID": {
        "type": "string",
        "title": "腾讯云 Secret ID",
        "description": "用于调用腾讯混元大模型的Secret ID"
      },
      "TENCENT_SECRET_KEY": {
        "type": "string",
        "title": "腾讯云 Secret Key",
        "description": "用于调用腾讯混元大模型的Secret Key"
      }
    }
  },
  "version": "1",
  "systemRole": "此插件使用腾讯混元大模型，根据文本提示生成图片。"
}